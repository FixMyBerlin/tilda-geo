    "@next/bundle-analyzer": "14.2.35",
    "@next/env": "14.2.35",
    "@placemarkio/check-geojson": "0.1.14",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/postcss": "^4.1.18",
