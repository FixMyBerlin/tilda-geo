{
  "name": "tilda-geo",
  "version": "0.2.0",
  "author": "FixMyCity <hello@fixmycity.de>",
  "license": "AGPL-3.0",
  "private": true,
  "engines": {
    "node": ">=24.0.0 <25.0.0"
  },
  "scripts": {
    "start": "npx blitz@2.2.4 start --hostname 127.0.0.1 --port 4000",
    "dev": "npx blitz@2.2.4 dev --hostname 127.0.0.1 --port 5173",
    "predev": "cp -f ../.env ./ && npm run predev:check_docker && npm run predev:check_migration",
    "predev:check_docker": "if [ \"$(docker inspect -f '{{.State.Running}}' db)\" != \"true\" ]; then docker compose -f ../docker-compose.yml up db tiles -d; fi",
    "predev:check_migration": "node scripts/CheckMigrations/check.js",
    "build": "npx blitz@2.2.4 build",
    "bleach": "rm -rf node_modules/ && rm -rf .next/ && npm install",
    "bleach:full": "rm package-lock.json && npm run bleach",
    "docker:cleanup": "docker system prune --force --all --volumes && docker builder prune --all --force",
    "studio": "npx blitz@2.2.4 prisma studio",
    "openTypes": "code \"node_modules/.prisma/client/index.d.ts\"",
    "migrate": "npx blitz@2.2.4 prisma migrate dev",
    "migrate:create": "npx blitz@2.2.4 prisma migrate dev --create-only && npm run migrate:create:openMigration",
    "migrate:create:openMigration": "code $(find ./db/migrations -type f -name \"migration.sql\" -cmin -1 | head -n 1)",
    "migrate:check": "npx blitz@2.2.4 prisma migrate status",
    "migrate:compare": "git diff origin/main... -- db/migrations",
    "seed": "npm run predev && npm run migrate && npx blitz@2.2.4 prisma migrate reset && npx blitz@2.2.4 db seed",
    "seed:full": "npm run seed && npm run StaticDatasets:update -- --env=dev",
    "type-check": "tsc --noEmit",
    "lint": "next lint",
    "format": "prettier --experimental-cli --write --ignore-path .gitignore \"src/**/*.{ts,js,tsx,md}\" \"mailers/**/*.{ts,js,tsx,md}\"",
    "regions": "npm run regions:masks && npm run regions:configs",
    "regions:masks": "npm run regions:masks:prepare && npm run regions:masks:format && npm run regions:masks:upload",
    "regions:masks:prepare": "bun ./scripts/StaticDatasets/createMasks.ts",
    "regions:masks:format": "prettier --experimental-cli --write \"scripts/StaticDatasets/geojson/masks/**/*.geojson\"",
    "regions:masks:upload": "bun ./scripts/StaticDatasets/updateStaticDatasets.ts --folder-filter=relation-",
    "regions:configs": "bun \"src/app/regionen/[regionSlug]/_hooks/useQueryState/useCategoriesConfig/v2/saveConfigs.ts\" && prettier --experimental-cli --write \"src/**/v2/configs/index.ts\" \"src/**/v2/configs/*.ts\"",
    "test": "vitest watch",
    "test:run": "vitest run --passWithNoTests",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "mailpreview": "NEXT_PUBLIC_APP_ORIGIN=http://127.0.0.1:5173 email dev --dir mailers",
    "check": "npm run type-check && npm run lint && npm run format && npm run test:run",
    "MapboxStyles:update": "bun ./scripts/MapboxStyles/process.ts && npm run lint -- --fix && prettier --experimental-cli --write \"src/app/regionen/[regionSlug]/_mapData/mapDataSubcategories/mapboxStyles\" && github",
    "StaticDatasets:link": "ln -sf ../../../../tilda-static-data/geojson scripts/StaticDatasets",
    "StaticDatasets:update": "bun ./scripts/StaticDatasets/updateStaticDatasets.ts",
    "StaticDatasets:updateDownloadSources": "bun --env-file=.env ./scripts/StaticDatasets/updateDownloadSources.ts --folder-filter=",
    "StaticDatasets:deleteAll:dev": "echo \"Reminder: We cannot use this on staging, production, see https://github.com/FixMyBerlin/private-issues/issues/2283\" && bun --env-file=.env --env-file=./scripts/StaticDatasets/.env.development ./scripts/StaticDatasets/deleteAllStaticDatasets.ts",
    "MapboxTilesets:update": "bun ./scripts/MapboxTilesets/process.ts",
    "TranslationAnalysis:update": "bun ./scripts/TranslationAnalysis/analyze.ts",
    "MaprouletteCreate": "bun run scripts/MaprouletteCreate/process.ts",
    "MaprouletteRebuild": "bun run scripts/MaprouletteRebuild/process.ts",
    "Maproulette:help": "echo -e '\\033[0;32m Help: Use `npm run MaprouletteCreate -- --filter FOOBAR` \\033[0m' && echo -e '\\033[0;32m Rebuild: Use `npm run MaprouletteRebuild -- --filter FOOBAR` \\033[0m'",
    "updatePackages:major": "npx taze major --includeLocked --write --exclude eslint,minimatch && npm run updatePackages:minor && npm install",
    "updatePackages:minor": "npx taze minor --includeLocked --write && npm install",
    "release": "gh pr create --base main --head develop --title \"Release $(date '+%Y-%m-%d')\" --body \"\"",
    "prepare": "cd .. && husky app/.husky",
    "postinstall": "patch-package"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "browserslist": [
    "defaults",
    "last 2 IE major versions",
    "last 4 Edge major versions"
  ],
  "dependencies": {
    "@blitzjs/auth": "2.2.4",
    "@blitzjs/next": "2.2.4",
    "@blitzjs/rpc": "2.2.4",
