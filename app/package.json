    "lodash": "4.17.21",
    "maplibre-gl": "4.7.1",
    "next": "14.2.35",
    "next-auth": "4.24.13",
    "node-mailjet": "6.0.11",
    "nuqs": "1.20.0",
    "pluralize": "8.0.0",
    "pmtiles": "3.2.1",
    "prisma": "5.22.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.71.1",
    "react-intl": "^6.8.9",
